-- Settings

local maponeset = {

    killaura = false,

    autofarm = false,

    autochest = false,

    autolily = false,

    killauratype = "Fist",

    bsldelay = 0.5,

    bslspeed = 250,

    selectedmobs = {},

    tspeed = 100,

    webhook = "",

    notify = false,

    ignoreitems = {},

    belowor = "Behind",

    studs = 10,

    autosoul = false

}

